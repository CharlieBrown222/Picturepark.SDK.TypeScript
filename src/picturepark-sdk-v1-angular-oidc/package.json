{
  "name": "@picturepark/sdk-v1-angular-oidc",
  "version": "0.1.5",
  "license": "MIT",
  "scripts": {
    "link": "npm link @picturepark/sdk-v1-angular",
    "init": "cmd /c if exist \"../picturepark-sdk-v1-angular/node_modules\" rmdir \"../picturepark-sdk-v1-angular/node_modules\" /s /q && npm i && npm run link",
    "start": "npm run init && ng serve",
    "build": "npm run init && ng-packagr -p ng-package.json",
    "test": "npm run init && ng test",
    "lint": "npm run init && ng lint",
    "e2e": "npm run init && ng e2e",
    "packagr": "ng-packagr -p ng-package.json"
  },
  "dependencies": {
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "core-js": "^2.4.1",
    "oidc-client": "^1.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^4.3.0",
    "@types/jasmine": "2.5.47",
    "@types/node": "~7.0.14",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.1",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.1",
    "ts-node": "~3.0.2",
    "tslint": "~5.1.0",
    "typescript": "~2.2.0"
  }
}
