{
  "name": "picturepark-sdk-v1-angular-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "link": "npm link @picturepark/sdk-v1-angular && npm link @picturepark/sdk-v1-angular-oidc && npm link @picturepark/sdk-v1-angular-ui",
    "init": "cmd /c if exist \"../picturepark-sdk-v1-angular/node_modules\" rmdir \"../picturepark-sdk-v1-angular/node_modules\" /s /q && cmd /c if exist \"../picturepark-sdk-v1-angular-oidc/node_modules\" rmdir \"../picturepark-sdk-v1-angular-oidc/node_modules\" /s /q && cmd /c if exist \"../picturepark-sdk-v1-angular-ui/node_modules\" rmdir \"../picturepark-sdk-v1-angular-ui/node_modules\" /s /q && npm i && npm run link",
    "start": "npm run init && ng serve --port 4500",
    "build": "npm run init && ng build --prod",
    "test": "npm run init && ng test",
    "lint": "npm run init && ng lint",
    "e2e": "npm run init && ng e2e"
  },
  "private": true,
  "dependencies": {
    "@picturepark/sdk-v1-angular": "*",
    "@picturepark/sdk-v1-angular-ui": "*",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "angular-auth-oidc-client": "^1.3.14",
    "angular2-virtual-scroll": "^0.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler-cli": "^4.3.0",
    "@types/jasmine": "2.5.47",
    "@types/node": "~7.0.14",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.1",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.1",
    "ts-node": "~3.0.2",
    "tslint": "~5.1.0",
    "typescript": "~2.2.0"
  }
}
